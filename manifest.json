{
  "manifest_version": 2,
  "name": "Multicolered Contributions",
  "version": "1.0",
  "description": "A simple chrome extension to change github contributions color",
  "permissions": ["tabs"],
  "browser_action": {
    "default_title": "",
    "default_popup": "popup.html"
  },
  "content_scripts" :[
    {
      "matches": ["https://github.com/*"],
      "js":["js/content.js"]
    }
  ]
}
